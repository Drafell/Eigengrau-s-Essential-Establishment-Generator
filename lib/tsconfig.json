{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2015",
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true ,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "lib": ["dom", "esnext"],
    "jsx": "react"
  },
  "exclude": ["node_modules", "gh-pages"]
}
